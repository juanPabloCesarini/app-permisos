@import "variables"

@font-face
  font-family 'icomoon'
  src url(icomoon-font-path + '/icomoon.eot?qf3lxn')
  src url(icomoon-font-path + '/icomoon.eot?qf3lxn#iefix') format('embedded-opentype'), url(icomoon-font-path + '/icomoon.ttf?qf3lxn') format('truetype'), url(icomoon-font-path + '/icomoon.woff?qf3lxn') format('woff'), url(icomoon-font-path + '/icomoon.svg?qf3lxn#icomoon') format('svg')
  font-weight normal
  font-style normal
  font-display block

.icon
  font-family 'icomoon' !important
  speak never
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

